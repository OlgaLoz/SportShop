@model List<MVC.ViewModels.Lot.LotViewModel>
    
<div class="table-responsive">
    <table class="table">
        @for (int i = 0; i < Model.Count(); i += 2)
            {
            <tr>
                @for (int j = i; j < i + 2 && j < Model.Count(); j++)
                {
                    <td>@Html.Partial("_SingleLotPartial", Model[j])</td>
                }
            </tr>
        }
    </table>
</div>

